{
  "name": "@programic/vue-router-middleware",
  "description": "Make complex route middleware logic look easy and readable!",
  "version": "1.0.1",
  "main": "./dist/index.js",
  "module": "./dist/index.cjs",
  "type": "module",
  "files": [
    "dist/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/programic/vue-route-middleware.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "router",
    "routes",
    "vue-router",
    "middleware"
  ],
  "contributors": [
    {
      "name": "Rick Bongers",
      "email": "r.bongers@programic.com"
    }
  ],
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "NODE_ENV=production vite build",
    "build:typescript": "vue-tsc -p tsconfig.build.json && mv dist/src dist/types",
    "test": "vitest"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^4.1.0",
    "vite": "^4.2.1",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@types/node": "^18.15.11",
    "@vue/tsconfig": "^0.1.3",
    "babel-cli": "^6.26.0",
    "babel-loader": "^9.1.2",
    "babel-preset-es2015": "^6.24.1",
    "jsdom": "^21.1.1",
    "typescript": "^4.9.5",
    "vitest": "^0.30.1",
    "vue-tsc": "^1.2.0"
  }
}
